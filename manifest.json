{
    "name": "TakobotoQuickSearch Chrome Extension",
    "description": "Allows one to open Takoboto on Highlighted Text in Browser",
    "author": "Houston Nguyen",
	"version": "1.0", 
    "manifest_version": 2,
	"permissions": [
        "<all_urls>",
        "http://*/*",
        "https://*/*",
        "contextMenus",
        "clipboardWrite",
        "tabs"
    ],
    "icons": {
        "16" : "/icons/icon16.png",
        "19" : "/icons/icon.png",
        "128" : "/icons/icon128.png"
    },
    "browser_action": {
        "default_title": "TakobotoQuickSearch Extension",
        "default_icon": "/icons/icon.png"
    },
    "background": {
    "persistent": false,
    "scripts": ["/background.js"]
    }
}
